<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.softwareengineering">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.CAMERA"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/orange"
        android:label="딥 러닝 기반 감귤 병해충 진단"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.manjoong.user.softwareengineering.SplashScreen"
            android:label = "@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.manjoong.user.softwareengineering.MenuActivity"></activity>
        <activity android:name="com.manjoong.user.softwareengineering.VisualSearchActivity"></activity>
        <activity android:name="com.manjoong.user.softwareengineering.KeySearchActivity"></activity>
        <activity android:name="com.manjoong.user.softwareengineering.DiseaseActivity"></activity>
    </application>

</manifest>